# Supabase Configuration
# These will be populated after running 'supabase start'
BACKEND_URL=http://127.0.0.1:54321
BACKEND_API_KEY=your_supabase_anon_key_here

# AI Providers
# Get your API keys from:
# - Anthropic: https://console.anthropic.com/
# - OpenAI: https://platform.openai.com/api-keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
AI_DEFAULT_PROVIDER=claude

# Email Verification (optional - for production)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password_here

# Twilio SMS (optional - for SMS auth)
SUPABASE_AUTH_SMS_ACCOUNT_SID=your_twilio_account_sid
SUPABASE_AUTH_SMS_MESSAGE_SERVICE_SID=your_twilio_message_service_sid
SUPABASE_AUTH_SMS_TWILIO_AUTH_TOKEN=your_twilio_auth_token

# S3 Storage (optional - for OrioleDB)
S3_HOST=
S3_REGION=
S3_ACCESS_KEY=
S3_SECRET_KEY=
